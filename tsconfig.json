{
    "compilerOptions": {
      "module": "CommonJS",
      "target": "es5",
      "lib": ["dom", "es6", "ES2020"],
      "jsx": "react",
      "noEmit": true,
      "sourceMap": true,
      "strict": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "paths": {
        "~api/*":["./src/api/*"],
        "~components/*":["./src/components/*"],
        "~hooks/*":["./src/hooks/*"]
      }
    },
    "include": ["src"],
  }